<div class="modal fade" id="addExistingExerciseModal" tabindex="-1" role="dialog" aria-labelledby="addExistingExerciseModal-label">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="addExistingExerciseModal-label">Add existing exercise to workout</h4>
            </div>
            <div class="modal-body">
                <form role="form" id="addExistingExerciseForm">
                    <div class="form-group">
                        <label class="control-label" for="">Name</label>
                        <p id="existingExerciseName"></p>
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="">Tool</label>
                        <p id="existingExerciseTool"></p>
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="">Description</label>
                        <p id="existingExerciseDescription"></p>
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="existingExerciseSets">Sets</label>
                        <input type="number" class="form-control" id="existingExerciseSets">
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="existingExerciseReps">Reps</label>
                        <input type="number" class="form-control" id="existingExerciseReps">
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="existingExerciseResistance">Resistance</label>
                        <input type="text" class="form-control" id="existingExerciseResistance">
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="existingExerciseNotes">Notes</label>
                        <textarea class="form-control" id="existingExerciseNotes"></textarea>
                    </div>
                    <button type="submit" class="btn btn-default">Submit</button>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>